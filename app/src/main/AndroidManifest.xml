<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rafyee.rafyee_rajiv.pstuprofile">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Developer.DeveloperWebview">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:scheme="skype" />
            </intent-filter>
        </activity>
        <activity android:name=".Website" />
        <activity android:name=".Dean.DeanDetails" />
        <activity android:name=".Dean.Dean" />
        <activity android:name=".ViceChancellor" />
        <activity android:name=".StudentProfile.StudentLogin" />
        <activity android:name=".MainActivity" />
        <activity android:name=".TeacherProfile.TeacherSignUp" />
        <activity android:name=".AllTeachers.AllTeachers" />
        <activity android:name=".TeacherProfile.TeacherUpdate" />
        <activity android:name=".TeacherProfile.TeacherLoggedIn" />
        <activity android:name=".TeacherProfile.TeacherLogin" />
        <activity android:name=".AllTeachers.TeacherDetails" />
        <activity android:name=".StudentProfile.StudentUpdate" />
        <activity android:name=".AllStudents.StudentDetails" />
        <activity android:name=".AllStudents.AllStudents" />
        <activity android:name=".StudentProfile.StudentLoggedIn" />
        <activity android:name=".StudentProfile.StudentSignUp" />
        <activity android:name=".Cources.CourseDetails" />
        <activity android:name=".Developer.Developer" />
        <activity android:name=".Cources.CourseFaculty" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>